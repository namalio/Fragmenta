SG SG_PVMI {
   node Person
   nodea Vehicle
   node Bicycle
   node Motorcycle
   node Car
   inh Bicycle->Vehicle
   inh Car->Vehicle
   inh Motorcycle->Vehicle
   virtual Motor
   inh Car->Motor
   inh Motorcycle->Motor
   node MotorInsurance
   rel Person->Vehicle[Owns]:1,*
   rel MotorInsurance->Motor[Insures]:0..1,1
}
