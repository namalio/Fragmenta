fragment F_PW4 {
  SG SG_PW4 {
    proxy PHotelRoom
    proxy PDog
    proxy PReptile
    proxy PCat
    enum HotelZone: dogs, reptiles, cats
    relu PHotelRoom->HotelZone[zone]
    derived PDog->dogs[S1 <-> Hosts~⨾zone]:*;1
    derived PReptile->reptiles[S2 <-> Hosts~⨾zone]:*;1
    derived PCat->cats[S3 <-> Hosts~⨾zone]:*;1
  }
  ref PHotelRoom->HotelRoom
  ref PDog->Dog
  ref PReptile->Reptile
  ref PCat->Cat
}
