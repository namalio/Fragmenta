fragment F_PW1 {
  SG SG_PW1 {
    virtual Named
    node Name
    nodea Pet
    node Date
    node PetOwner
    virtual Locatable
    node Country
    node City
    node PHCard
    relu Named->Name[Named_name]
    relu Locatable->Country[Locatable_country]
    relu Locatable->City[Locatable_city]
    relu Pet->Date[Pet_dob]
    rel PetOwner->Pet[Owns]:1;*
    rel PetOwner->PHCard[Holds]:1;*
    inh Pet->Named
    inh PetOwner->Named
    inh PetOwner->Locatable
  }
}
