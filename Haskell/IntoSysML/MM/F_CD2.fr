fragment F_CD2 {
  SG SG_CD2 {
  	proxy PNamed2
    node PortI
    proxy PBlockI
    proxy PConnector
    compu PBlockI->PortI[BlockI_ports]:*
    inh PortI->PNamed2
    relu PConnector->PortI[Connector_src]
    relu PConnector->PortI[Connector_tgt]
  }
  ref PNamed2->Named
  ref PBlockI->BlockI
  ref PConnector->Connector
  iON PortI->Port
  iOE BlockI_ports->Block_ports
}
