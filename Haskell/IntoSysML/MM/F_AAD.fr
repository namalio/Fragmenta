fragment F_AAD {
  SG SG_AAD {
    node ArchitectureDiagram
    virtual Trait
    nodea PElement
    node VType
    node Part
    node Connector
    node Feature
    compu ArchitectureDiagram->PElement[ConsistsOf]:*
    rel Trait->Trait[Has]:*;*
    inh ArchitectureDiagram->Trait
    inh PElement->Trait
    inh Feature->Trait
    inh Part->PElement
    inh Connector->PElement
    inh VType->PElement
    relu Connector->Part[Connector_src]
    relu Connector->Part[Connector_tgt]
  }
}
