HouseLiving = 
   let 
      EnterHall = intoHall -> (EnterRoom
            [] intoKitchen -> EnterHall
            [] intoLivingRoom -> EnterHall
            [] mainDoor -> HouseLiving)
      EnterRoom = intoRoom -> (EnterHall
            [] roomWindow -> HouseLiving)
   within
      mainDoor -> EnterHall
         [] roomWindow -> EnterRoom